{
  "name": "awcentral-client",
  "version": "0.1.0-alpha+0001",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@react-keycloak/web": "^3.4.0",
    "@testing-library/user-event": "^13.5.0",
    "amplitude-js": "^8.2.3",
    "apollo-link-http": "^1.5.17",
    "dayjs": "^1.10.3",
    "fontfaceobserver": "^2.1.0",
    "graphql": "^16.3.0",
    "grommet": "2.17.0",
    "grommet-icons": "^4.4.0",
    "keycloak-js": "^11.0.3",
    "lodash": "^4.17.20",
    "polished": "^4.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^6.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.1.1",
    "utility-types": "^3.10.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom",
    "eject": "react-scripts eject",
    "start:dev": "env-cmd -f ./src/config/dev.env yarn start",
    "build:staging": "env-cmd -f ./src/config/staging.env yarn run build",
    "build:prod": "env-cmd -f ./src/config/prod.env yarn run build",
    "get-schema": "apollo client:download-schema --endpoint=http://localhost:8080/graphql introspection.schema.graphql",
    "cypress:open": "cypress open",
    "cypress:all": "cypress run",
    "test-coverage": "yarn test --coverage --watchAll=false && yarn cypress:all && node ./scripts/mergeCoverage.js",
    "prettier:check": "yarn prettier --check .",
    "prettier:write": "yarn prettier --write ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/amplitude-js": "^8.0.0",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/sockjs-client": "^1.1.1",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "apollo": "^2.33.9",
    "cypress": "^9.4.1",
    "cypress-graphql-mock-network": "^0.13.0",
    "cypress-keycloak-commands": "^1.2.0",
    "env-cmd": "^10.1.0",
    "jest-environment-jsdom": "^27.5.1",
    "prettier": "^2.3.2",
    "typescript": "^4.1.3",
    "waait": "^1.0.5"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "nyc": {
    "report-dir": "coverage-cypress",
    "exclude": [
      "**/LandingPage.tsx"
    ]
  },
  "jest": {
    "coverageReporters": [
      "json"
    ]
  }
}
